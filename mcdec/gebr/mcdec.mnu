    <parameters default-selection="0" selection="">
      <parameter>
        <label>Number of traces</label>
        <int>
          <property required="yes">
            <keyword>--ntr </keyword>
            <value/>
            <default/>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Number of channels</label>
        <int>
          <property required="yes">
            <keyword>--nc </keyword>
            <value/>
            <default>1</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Number of selected trace to be deconvolved</label>
        <int>
          <property required="yes">
            <keyword>--jtarget </keyword>
            <value/>
            <default>1</default>
          </property>
        </int>
      </parameter>
      <parameter>
        <label>Multiple period file</label>
        <file directory="no">
          <property required="yes">
            <keyword>--pfile </keyword>
            <value></value>
            <default/>
          </property>
        </file>
      </parameter>
      <parameter>
        <label>Percentage of the period</label>
        <range min="0" max="100" inc="0.1" digits="1">
          <property required="no">
            <keyword>--perc </keyword>
            <value/>
            <default>0.2</default>
          </property>
        </range>
      </parameter>
    </parameters>
  </program>
