    <parameters default-selection="0" selection="">
      <parameter>
        <label>Minimum x coordinate (left corner)</label>
        <float>
          <property required="no">
            <keyword>--xmin=</keyword>
            <value/>
            <default/>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Maximum x coordinate (right corner)</label>
        <float>
          <property required="yes">
            <keyword>--xmax=</keyword>
            <value/>
            <default>6</default>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Minimum z coordinate</label>
        <float>
          <property required="no">
            <keyword>--zmin=</keyword>
            <value/>
            <default/>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Maximum z coordinate</label>
        <float>
          <property required="no">
            <keyword>--zmax=</keyword>
            <value/>
            <default/>
          </property>
        </float>
      </parameter>
      <parameter>
        <label>Turn off filling in of layers</label>
        <flag>
          <property required="no">
            <keyword>--nofill</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Land color palette</label>
        <flag>
          <property required="no">
            <keyword>--land</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter><label>File containing a custom palette</label><file directory="no" filter-name="" filter-pattern=""><property required="no"><keyword>--palette=</keyword><value/><default/></property></file></parameter><parameter>
        <label>Display all rays in black</label>
        <flag>
          <property required="no">
            <keyword>--blackrays</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
      <parameter>
        <label>Suppress rays</label>
        <flag>
          <property required="no">
            <keyword>--norays</keyword>
            <value>off</value>
            <default>off</default>
          </property>
        </flag>
      </parameter>
    <parameter><label>Export velocity model and interfaces</label><group instanciable="no" expand="no"><template-instance><parameters default-selection="0"><parameter><label>File to read top and bottom velocities for each layer</label><file directory="no" filter-name="" filter-pattern=""><property required="no"><keyword>--layervel=</keyword><value/><default>&lt;TMP&gt;/vel.rays2</default></property></file></parameter><parameter><label>File to export the velocity model</label><file directory="no" filter-name="ASCII file" filter-pattern="*.dat"><property required="no"><keyword>--vfile=</keyword><value/><default/></property></file></parameter><parameter><label>File to export interfaces</label><file directory="no" filter-name="ASCII file" filter-pattern="*.dat"><property required="no"><keyword>--interfaces=</keyword><value/><default/></property></file></parameter><parameter><label>Number samples in x (for velocity model and interfaces)</label><int><property required="no"><keyword>--nx=</keyword><value/><default>101</default></property></int></parameter><parameter><label>Number samples in z (for velocity model)</label><int><property required="no"><keyword>--nz=</keyword><value/><default>101</default></property></int></parameter></parameters></template-instance><parameters default-selection="0"><parameter><label>File to export the velocity model</label><reference><property required="no"><keyword/><value/><default>&lt;TMP&gt;/vel.rays2</default></property></reference></parameter><parameter><label/><reference><property required="no"><keyword/><value/><default/></property></reference></parameter><parameter><label>File to read top and bottom velocities for each layer</label><reference><property required="no"><keyword/><value/><default/></property></reference></parameter><parameter><label>Number samples in x</label><reference><property required="no"><keyword/><value/><default>101</default></property></reference></parameter><parameter><label>Number samples in z</label><reference><property required="no"><keyword/><value/><default>101</default></property></reference></parameter></parameters></group></parameter></parameters>
  </program>
